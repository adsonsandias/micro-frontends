{
  "version": 2,
  "buildCommand": "cd ../.. && npm ci --workspace=@repo/dashboard && npm run build --workspace=@repo/dashboard",
  "outputDirectory": "dist",
  "installCommand": "cd ../.. && npm ci --workspace=@repo/dashboard",
  "framework": "vite",
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}